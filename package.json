		"@angular/forms": "^20.1.6",
		"@angular/material": "20.1.5",
		"@angular/platform-browser": "^20.1.6",
		"@angular/platform-browser-dynamic": "^20.1.6",
		"@angular/router": "^20.1.6",
		"@capacitor/android": "^6.2.1",
		"@capacitor/app": "^6.0.3",
		"@capacitor/core": "^6.2.1",
		"@capacitor/ios": "^6.2.1",
		"@ionic/angular": "^8.7.17",
		"@ngrx/signals": "^20.0.0",
		"@tailwindcss/postcss": "^4.1.11",
		"ag-charts-angular": "^12.2.0",
		"ag-charts-community": "^12.2.0",
		"gsap": "^3.13.0",
		"lottie-web": "^5.13.0",
		"matter-js": "^0.20.0",
		"ngx-lottie": "^20.0.0",
		"postcss": "^8.5.6",
		"rxjs": "~7.8.2",
