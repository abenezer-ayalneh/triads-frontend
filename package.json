{
	"name": "triads",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"lint": "ng lint --fix",
		"format": "prettier --write \"src/**/*.{ts,js,html,css,scss}\"",
		"pretty-quick": "pretty-quick --staged",
		"lint-staged": "lint-staged",
		"prepare": "husky",
		"ionic:build": "ng build --configuration production",
		"ionic:serve": "ng serve",
		"cap:sync": "cap sync",
		"cap:ios": "cap open ios",
		"cap:android": "cap open android",
		"cap:add:ios": "cap add ios",
		"cap:add:android": "cap add android",
		"cap:dev:ios": "ng build --configuration development && cap sync && cap run ios",
		"cap:dev:android": "ng build --configuration development && cap sync && cap run android"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^20.1.6",
		"@angular/cdk": "20.1.5",
		"@angular/common": "^20.1.6",
